{
  "name": "typescript_node_server",
  "version": "1.0.2",
  "description": "node & typescript starter project",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev index.ts",
    "dist": "rimraf dist && tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest --detectOpenHandles --colors",
    "lint-and-fix": "eslint . --ext .ts --fix --cache",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himanshuteotia/typescript_node_server.git"
  },
  "author": "Himanshu Teotia",
  "license": "ISC",
  "keywords": [
    "typescript",
    "node",
    "node starter",
    "typescript starter"
  ],
  "bugs": {
    "url": "https://github.com/himanshuteotia/typescript_node_server/issues"
  },
  "homepage": "https://github.com/himanshuteotia/typescript_node_server#readme",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lusca": "^1.7.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "lusca": "^1.7.0",
    "luxon": "^3.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "node-cache": "^5.1.2",
    "otplib": "^12.0.1",
    "smartapi-javascript": "^1.0.14",
    "winston": "^3.9.0"
  },
  "hooks": {
    "pre-commit": "pretty-quick --staged --bail && lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint-and-fix"
    ]
  }
}
